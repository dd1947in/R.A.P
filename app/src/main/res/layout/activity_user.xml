<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:ads="http://schemas.android.com/apk/res-auto"
    android:layout_width="386dp"
    android:layout_height="386dp"
    android:orientation="vertical"
    tools:context="com.example.uadnd.cou8901.rap.ui.UserActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="24dp"
        android:orientation="horizontal"
        android:weightSum="1">

        <TextView
            android:id="@+id/tv_user_property_label"
            android:layout_width="100dp"
            android:layout_height="match_parent"
            android:layout_weight="0.33"
            android:gravity="left"
            android:text="Property Name"
            android:textColor="@color/colorPrimary"
            android:textSize="16sp"
            android:textStyle="bold"
            tools:textColor="@color/colorPrimaryDark" />

        <TextView
            android:id="@+id/tv_user_property_value"
            android:layout_width="200dp"
            android:layout_height="match_parent"
            android:layout_weight="0.67"
            android:text="Property Value"
            android:textColor="@color/colorPrimaryDark"
            android:textSize="16sp"
            android:textStyle="bold" />
    </LinearLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="24dp"
        android:orientation="horizontal"
        android:weightSum="1">

        <TextView
            android:id="@+id/tv_id_label"
            android:layout_width="100dp"
            android:layout_height="match_parent"
            android:layout_weight="0.33"
            android:textColor="@color/colorPrimary"
            android:gravity="left"
            android:textSize="12sp"
            android:text="label1" />

        <TextView
            android:id="@+id/tv_id_value"
            android:layout_width="200dp"
            android:layout_weight="0.67"
            android:layout_height="match_parent"
            android:textColor="@color/colorPrimary"
            android:textSize="12sp"
            android:text="value1" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="24dp"
        android:orientation="horizontal"
        android:weightSum="1">

        <TextView
            android:id="@+id/tv_name_label"
            android:layout_width="100dp"
            android:layout_height="match_parent"
            android:layout_weight="0.33"
            android:textColor="@color/colorPrimary"
            android:gravity="left"
            android:text="label1" />

        <TextView
            android:id="@+id/tv_name_value"
            android:layout_width="200dp"
            android:layout_weight="0.67"
            android:textColor="@color/colorPrimary"
            android:layout_height="match_parent"
            android:textSize="12sp"
            android:text="value1" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="24dp"
        android:orientation="horizontal"
        android:weightSum="1">

        <TextView
            android:id="@+id/tv_created_label"
            android:layout_width="100dp"
            android:layout_height="match_parent"
            android:layout_weight="0.33"
            android:textColor="@color/colorPrimary"
            android:gravity="left"
            android:text="label1" />

        <TextView
            android:id="@+id/tv_created_value"
            android:layout_width="200dp"
            android:layout_weight="0.67"
            android:textColor="@color/colorPrimary"
            android:layout_height="match_parent"
            android:textSize="12sp"
            android:text="value1" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="24dp"
        android:orientation="horizontal"
        android:weightSum="1">

        <TextView
            android:id="@+id/tv_created_utc_label"
            android:layout_width="100dp"
            android:layout_height="match_parent"
            android:layout_weight="0.33"
            android:textSize="12sp"
            android:textColor="@color/colorPrimary"
            android:gravity="left"
            android:text="label1" />

        <TextView
            android:id="@+id/tv_created_utc_value"
            android:layout_width="200dp"
            android:layout_weight="0.67"
            android:textSize="12sp"
            android:textColor="@color/colorPrimary"
            android:layout_height="match_parent"
            android:text="value1" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="24dp"
        android:orientation="horizontal"
        android:weightSum="1">

        <TextView
            android:id="@+id/tv_over_18_label"
            android:layout_width="100dp"
            android:layout_height="match_parent"
            android:layout_weight="0.33"
            android:textSize="12sp"
            android:textColor="@color/colorPrimary"
            android:gravity="left"
            android:text="label1" />

        <TextView
            android:id="@+id/tv_over_18_value"
            android:layout_width="200dp"
            android:layout_weight="0.67"
            android:textSize="12sp"
            android:textColor="@color/colorPrimary"
            android:layout_height="match_parent"
            android:text="value1" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="24dp"
        android:orientation="horizontal"
        android:weightSum="1">

        <TextView
            android:id="@+id/tv_has_verified_email_label"
            android:layout_width="100dp"
            android:layout_height="match_parent"
            android:layout_weight="0.33"
            android:textColor="@color/colorPrimary"
            android:textSize="12sp"
            android:gravity="left"
            android:text="label1" />

        <TextView
            android:id="@+id/tv_has_verified_email_value"
            android:layout_width="200dp"
            android:layout_weight="0.67"
            android:textSize="12sp"
            android:textColor="@color/colorPrimary"
            android:layout_height="match_parent"
            android:text="value1" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="24dp"
        android:orientation="horizontal"
        android:weightSum="1">

        <TextView
            android:id="@+id/tv_oauth_client_id_label"
            android:layout_width="100dp"
            android:layout_height="match_parent"
            android:layout_weight="0.33"
            android:textColor="@color/colorPrimary"
            android:gravity="left"
            android:textSize="12sp"
            android:text="label1" />

        <TextView
            android:id="@+id/tv_oauth_client_id_value"
            android:layout_width="200dp"
            android:layout_weight="0.67"
            android:textSize="12sp"
            android:textColor="@color/colorPrimary"
            android:layout_height="match_parent"
            android:text="value1" />
    </LinearLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="24dp"
        android:orientation="horizontal"
        android:weightSum="1">

        <TextView
            android:id="@+id/tv_token_issue_time_label"
            android:layout_width="100dp"
            android:layout_height="match_parent"
            android:layout_weight="0.33"
            android:textSize="12sp"
            android:textColor="@color/colorPrimary"
            android:gravity="left"
            android:text="label1" />

        <TextView
            android:id="@+id/tv_token_issue_time_value"
            android:layout_width="200dp"
            android:layout_weight="0.67"
            android:textSize="12sp"
            android:textColor="@color/colorPrimary"
            android:layout_height="match_parent"
            android:text="value1" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="24dp"
        android:orientation="horizontal"
        android:weightSum="1">

        <TextView
            android:id="@+id/tv_token_expire_time_label"
            android:layout_width="100dp"
            android:layout_height="match_parent"
            android:layout_weight="0.33"
            android:textSize="12sp"
            android:textColor="@color/colorPrimary"
            android:gravity="left"
            android:text="label1" />

        <TextView
            android:id="@+id/tv_token_expire_time_value"
            android:layout_width="200dp"
            android:layout_weight="0.67"
            android:textSize="12sp"
            android:textColor="@color/colorPrimary"
            android:layout_height="match_parent"
            android:text="value1" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="36dp"
        android:orientation="horizontal"
        android:weightSum="1">

        <TextView
            android:id="@+id/tv_access_token_label"
            android:layout_width="100dp"
            android:layout_height="match_parent"
            android:layout_weight="0.33"
            android:textSize="12sp"
            android:textColor="@color/colorPrimary"
            android:gravity="left"
            android:text="label1" />

        <TextView
            android:id="@+id/tv_access_token_value"
            android:layout_width="200dp"
            android:layout_weight="0.67"
            android:textSize="12sp"
            android:textColor="@color/colorPrimary"
            android:layout_height="match_parent"
            android:text="value1" />
    </LinearLayout>

    <!--  Adview at the bottom-->

    <com.google.android.gms.ads.AdView
        android:id="@+id/adView"
        android:layout_width="359dp"
        android:layout_height="67dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        ads:adSize="BANNER"
        ads:adUnitId="@string/banner_ad_unit_id"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/scrollView2"></com.google.android.gms.ads.AdView>


</LinearLayout>

